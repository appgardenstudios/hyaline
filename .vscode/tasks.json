  "version": "2.0.0",
  "tasks": [
    {
      "label": "cleanup",
      "type": "shell",
      "command": "rm -rf current.db change.db",
      "options":{
        "cwd": "${workspaceFolder}/cli/"
      },
      "presentation":{
        "reveal": "silent"
      }
    },{
      "label": "cleanup-generate",
      "type": "shell",
      "command": "rm -f generated-config.yml",
      "options":{
        "cwd": "${workspaceFolder}/cli/"
      },
      "presentation":{
        "reveal": "silent"
      }
    },{
      "label": "cleanup-check",
      "type": "shell",
      "command": "rm -f results.json",
      "options":{
        "cwd": "${workspaceFolder}/cli/"
      },
      "presentation":{
        "reveal": "silent"
      }
    },{
      "label": "cleanup-update-pr",
      "type": "shell",
      "command": "rm -f comment.json",
      "options":{
        "cwd": "${workspaceFolder}/cli/"
      },
      "presentation":{
        "reveal": "silent"
      }
    },{
      "label": "extract-documentation-fs",
      "type": "shell",
      "command": "rm -f ../.vscode/hyaline/extract-documentation-fs/documentation.db",
      "options":{
        "cwd": "${workspaceFolder}/cli/"
      },
      "presentation":{
        "reveal": "silent"
      }
    }
  ]
}