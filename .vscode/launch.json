      "envFile": "${workspaceFolder}/cli/.env",
      "args":[
        "--debug",
        "generate", "config",
        "--config", "./_example/config.yml",
        "--current", "./current.db",
        "--system", "generate-config",
        "--output", "./generated-config.yml"
      ]
