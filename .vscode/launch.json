  "version": "0.2.0",
  "configurations": [
    {
      "name": "Extract Current",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "preLaunchTask": "cleanup",
      "cwd": "${workspaceFolder}/cli/",
      "program": "${workspaceFolder}/cli/cmd/hyaline.go",
      "envFile": "${workspaceFolder}/cli/.env",
      "args":[
        "--debug", "extract", "current", "--config", "./_example/config.yml", "--system", "local", "--output", "./current.db"
      ]
    },{
      "name": "Extract Current (Git)",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "preLaunchTask": "cleanup",
      "cwd": "${workspaceFolder}/cli/",
      "program": "${workspaceFolder}/cli/cmd/hyaline.go",
      "envFile": "${workspaceFolder}/cli/.env",
      "args":[
        "--debug", "extract", "current", "--config", "./_example/config.yml", "--system", "git", "--output", "./current.db"
      ]
    }
    
  ]
}